{
  "name": "pagopa-wallet-be-mock",
  "version": "0.0.1",
  "description": "TODO",
  "repository": "https://github.com/pagopa/pagopa-wallet-be-mock",
  "author": "Fernando Granato",
  "license": "MIT",
  "scripts": {
    "dev": "./node_modules/nodemon/bin/nodemon.js",
    "ts-node": "export $(grep -v '^#' .env | xargs) && ts-node src/index.ts",
    "start": "npm-run-all -s clean lint build:* && export $(grep -v '^#' .env | xargs) && node dist/index.js",
    "prebuild": "rimraf dist",
    "build": "npm-run-all -s clean lint build:*",
    "build:tsc": "tsc",
    "postversion": "git push && git push --tags",
    "test": "jest",
    "lint": "eslint . -c .eslintrc.js --ext .ts,.tsx --cache",
    "lint-autofix": "eslint . -c .eslintrc.js --ext .ts,.tsx --fix",
    "clean": "npm-run-all -s clean:*",
    "clean:dist": "rimraf dist",
    "clean:yaml-files": "rimraf generated/yaml-file/*.yaml",
    "generate": "npm-run-all -s generate:ts-from-yaml:*",
    "generate:ts-from-yaml:wallet": "shx rm -rf src/generated/wallet && shx mkdir -p src/generated/wallet && gen-api-models --api-spec https://raw.githubusercontent.com/pagopa/pagopa-infra/v1.327.0/src/domains/pay-wallet-app/api/webview-payment-wallet/v1/_openapi.json.tpl --no-strict --out-dir src/generated/wallet"
  },
  "devDependencies": {
    "@pagopa/eslint-config": "^1.3.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.6",
    "@types/express-mung": "^0.5.2",
    "@types/jest": "^25.2.1",
    "@types/node": "^14.16.0",
    "@types/uuid": "^9.0.0",
    "@types/winston": "^2.4.4",
    "danger": "^8.0.0",
    "danger-plugin-digitalcitizenship": "*",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^25.2.7",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.12.1",
    "rimraf": "^2.6.2",
    "shx": "^0.3.3",
    "ts-jest": "^25.3.1",
    "ts-node": "^10.9.1",
    "tslint": "^5.1.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@pagopa/openapi-codegen-ts": "^12.0.1",
    "@pagopa/ts-commons": "^10.2.0",
    "@types/node-fetch": "^2.6.2",
    "cookie-parser": "^1.4.6",
    "express": "^4.17.1",
    "express-interceptor": "^1.2.0",
    "fp-ts": "^2.11.1",
    "hijackresponse": "^5.0.0",
    "http-proxy-middleware": "^2.0.1",
    "io-ts": "^2.2.16",
    "io-ts-reporters": "^2.0.1",
    "node-fetch": "2",
    "parcel-bundler": "^1.12.5",
    "request": "^2.88.2",
    "uuid": "^3.4.0",
    "winston": "^3.2.1"
  }
}
